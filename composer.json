{
    "name": "eduzz/judas",
    "description": "Essa Lib é responsável por salvar os logs das aplicações Eduzz.",
    "type": "library",
    "keywords": ["library", "elasticsearch", "log", "eduzz"],
    "autoload": {
        "psr-4": {
            "Eduzz\\Judas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eduzz\\Judas\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "php": ">=5.4",
        "mockery/mockery": "^1.0"
    },
    "require": {
        "php": ">=5.4",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "adbario/php-dot-notation": "2.2.0"
    }
}
