{
    "name": "eduzz/judas",
    "description": "Essa Lib é responsável por salvar os logs das aplicações Eduzz.",
    "type": "library",
    "keywords": ["library", "elasticsearch", "log", "eduzz"],
    "autoload": {
        "psr-4": {
            "Eduzz\\Judas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eduzz\\Judas\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "require": {
        "php": ">=5.5",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "adbario/php-dot-notation": "^2.0",
        "eduzz/hermes": "dev-master",
        "guzzlehttp/guzzle": "^6.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:eduzz/hermes.git"
        }
    ]
}
